{
  "name": "@chromatica/api",
  "version": "5.0.0",
  "private": true,
  "description": "Express API for Chromatica",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "dotenvx run -f .env.dev -- tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@chromatica/shared": "*",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "redis": "^5.9.0",
    "uploadthing": "^7.7.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  }
}
