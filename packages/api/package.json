{
  "name": "@chromatica/api",
  "version": "5.0.0",
  "private": true,
  "description": "Express API for Chromatica",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.mjs",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@chromatica/shared": "*",
    "express": "^4.21.1",
    "uploadthing": "^6.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "eslint": "^9.32.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  }
}
